<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  
  <servlet>
  	<servlet-name>Permanent</servlet-name>
  	<display-name>Permanent</display-name>
  	<description></description>
  	<servlet-class>controller.Permanent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Contract</servlet-name>
  	<display-name>Contract</display-name>
  	<description></description>
  	<servlet-class>controller.Contract</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Parttime</servlet-name>
  	<display-name>Parttime</display-name>
  	<description></description>
  	<servlet-class>controller.Parttime</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AllEmp</servlet-name>
  	<display-name>AllEmp</display-name>
  	<description></description>
  	<servlet-class>controller.AllEmp</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Demo</servlet-name>
  	<display-name>Demo</display-name>
  	<description></description>
  	<servlet-class>controller.Demo</servlet-class>
  </servlet>

  
  

  <servlet-mapping>
  	<servlet-name>Permanent</servlet-name>
  	<url-pattern>/Permanent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Contract</servlet-name>
  	<url-pattern>/Contract</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Parttime</servlet-name>
  	<url-pattern>/Parttime</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AllEmp</servlet-name>
  	<url-pattern>/AllEmp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Demo</servlet-name>
  	<url-pattern>/Demo</url-pattern>
  </servlet-mapping>

</web-app>
